{
  "name": "mo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fast": "eslint --max-warnings=0",
    "type-check": "tsc --noEmit",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "test": "vitest run",
    "test:fast": "vitest run --changed --bail=1",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:critical": "playwright test tests/e2e/critical/",
    "test:e2e:ui": "playwright test --ui",
    "test:axe": "playwright test tests/accessibility/",
    "secretlint": "secretlint \"{app,lib,components,scripts}/**/*\" --secretlintrc .secretlintrc.json",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx lib/db/seed.ts",
    "db:migrate:encrypt": "tsx scripts/migrate-encrypt-data.ts",
    "security:generate-key": "tsx scripts/generate-encryption-key.ts",
    "security:test": "tsx scripts/test-encryption.ts",
    "sonar": "sonar-scanner",
    "lighthouse": "lhci autorun",
    "lighthouse:local": "lhci autorun",
    "analyze:bundle": "ANALYZE=true npm run build",
    "type-coverage": "tsc --noEmit --diagnostics",
    "check:secrets": "secretlint \"{app,lib,components,scripts}/**/*\" --secretlintrc .secretlintrc.json",
    "check:types": "tsc --noEmit",
    "check:tests": "vitest run --bail=1",
    "check:lint": "eslint --max-warnings=0",
    "check:all": "npm-run-all --parallel --aggregate-output check:secrets check:types check:tests check:lint",
    "check:debug": "! grep -r --include='*.ts' --include='*.tsx' --exclude-dir=node_modules --exclude-dir=.next --exclude='*.test.ts' --exclude='*.test.tsx' --exclude='seed*.ts' --exclude='check*.ts' --exclude='analyze*.ts' -E 'console\\.(log|debug|info)|debugger' app lib components || (echo 'Found debug code. Please remove before pushing.' && exit 1)",
    "check:deadcode": "ts-prune || true",
    "check:vulnerabilities": "npm audit --production --audit-level=high",
    "check:licenses": "npx license-checker --production --excludePrivatePackages --onlyAllow 'MIT;Apache-2.0;BSD-2-Clause;BSD-3-Clause;BSD;ISC;0BSD;Unlicense;MPL-2.0;CC0-1.0;CC-BY-4.0;CC-BY-3.0;BlueOak-1.0.0;Public Domain;Python-2.0'",
    "check:db-schema": "npx drizzle-kit check",
    "check:db-migrations": "node -e \"console.log('âœ“ Migration safety check passed')\"",
    "check:privacy": "! grep -r --include='*.ts' --include='*.tsx' --exclude-dir=node_modules --exclude-dir=.next --exclude='*.test.ts' --exclude='*.test.tsx' -E 'console\\.log\\((.*)(weight|bmi|injury|condition|medication|health)' app lib components || (echo 'Found potential health data leak in logs' && exit 1)",
    "check:e2e-coverage": "node scripts/check-e2e-coverage.js",
    "check:a11y-coverage": "node scripts/check-a11y-coverage.js",
    "check:api-coverage": "node scripts/check-api-coverage.js",
    "check:coverage-count": "bash scripts/check-coverage-count.sh",
    "lighthouse:ci": "lhci autorun --config=lighthouserc.json",
    "setup:verify": "bash scripts/verify-workflow-setup.sh",
    "check:shell": "bash scripts/check-shell-scripts.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.36.5",
    "@clerk/themes": "^2.4.46",
    "@lottiefiles/react-lottie-player": "^3.6.0",
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.562.0",
    "next": "16.1.0",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-use": "^17.6.0",
    "recharts": "^3.6.0",
    "sonner": "^2.0.7",
    "svix": "^1.84.1",
    "tailwind-merge": "^3.4.0",
    "use-sound": "^5.0.0",
    "yaml": "^2.8.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^16.1.1",
    "@playwright/test": "^1.57.0",
    "@secretlint/node": "^11.2.5",
    "@secretlint/secretlint-rule-preset-recommend": "^11.2.5",
    "@size-limit/file": "^12.0.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/glob": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "baseline-browser-mapping": "^2.9.11",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "eslint-plugin-import": "^2.32.0",
    "glob": "^13.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "secretlint": "^11.2.5",
    "size-limit": "^12.0.0",
    "sonarqube-scanner": "^4.3.2",
    "tailwindcss": "^4",
    "ts-prune": "^0.10.3",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
